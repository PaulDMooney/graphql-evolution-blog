{
  "name": "typescript-babel-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run type-check && babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "jest": "jest",
    "nodemon": "nodemon -r dotenv/config",
    "nodemon:debug": "nodemon --inspect -r dotenv/config",
    "lint": "tsc --noEmit && eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" --quiet --fix",
    "lint-staged": "lint-staged",
    "run:js": "node -r dotenv/config dist/index",
    "start": "npm run nodemon:debug",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --detectOpenHandles --forceExit --watch a",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.6",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "tsconfig-paths-jest": "0.0.1",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "apollo-server": "^2.14.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.0.0"
  }
}
